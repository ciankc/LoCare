<!DOCTYPE html>
<html>
  <head>
    <title>Locare</title>
    <meta name="viewport" content="initial-scale=1.0">
    <meta charset="utf-8">
    <link rel="stylesheet" href="stylesheets/style.css">
  </head>
  <body>
    <input id="pac-input" class="controls" type="text" placeholder="Search Box">

    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 34.0522, lng: -118.2437},
          zoom: 11
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCNj-YsCIOTmbGfofgV6o4D8Ct39hVAbqI&callback=initMap"
    async defer></script>
  </body>
</html>